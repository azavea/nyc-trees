{
  "name": "nyc-trees",
  "version": "0.0.0",
  "description": "New York City Tree Census 2015",
  "main": "gulpfile.js",
  "browserify": {
    "transform": [
      "browserify-shim"
    ]
  },
  "browser": {
    "bootstrap": "./node_modules/bootstrap/dist/js/bootstrap.js",
    "bootstrap-datetimepicker": "./js/shim/bootstrap-datetimepicker.js",
    "modernizr": "./js/shim/modernizr.js",
    "leaflet-utfgrid": "./js/shim/leaflet.utfgrid-src.js",
    "select2": "./js/shim/select2.js"
  },
  "browserify-shim": {
    "bootstrap": {
      "depends": [
        "jquery:jQuery"
      ]
    },
    "modernizr": "Modernizr",
    "leaflet-utfgrid": {
      "depends": [
        "leaflet:L"
      ]
    },
    "select2": {
      "exports": "Select2",
      "depends": [
        "jquery:jQuery"
      ]
    }
  },
  "dependencies": {
    "bootstrap": "^3.3.1",
    "handlebars": "^3.0.0",
    "jquery": "^2.1.1",
    "leaflet": "^0.7.3",
    "leaflet-draw": "^0.2.3",
    "leaflet-geometryutil": "^0.3.2",
    "moment": "^2.8.4",
    "toastr": "^2.0.4"
  },
  "devDependencies": {
    "autoprefixer-core": "^5.1.11",
    "browserify": "^9.0.4",
    "browserify-shim": "^3.8.3",
    "chai": "^1.10.0",
    "csswring": "^3.0.4",
    "del": "^0.1.3",
    "factor-bundle": "^2.3.3",
    "gulp": "~3.8.8",
    "gulp-concat": "~2.4.1",
    "gulp-if": "~1.2.5",
    "gulp-jshint": "^1.9.0",
    "gulp-livereload": "~2.1.1",
    "gulp-postcss": "^5.1.3",
    "gulp-rev-all": "~0.6.6",
    "gulp-ruby-sass": "^1.0.5",
    "gulp-shell": "^0.2.10",
    "gulp-sourcemaps": "~1.2.4",
    "gulp-uglify": "~1.0.1",
    "gulp-util": "~3.0.1",
    "merge-stream": "~0.1.6",
    "minimist": "~1.1.0",
    "mocha": "^2.0.1",
    "orchestrator": "~0.3.7",
    "run-sequence": "^1.0.2",
    "sinon": "^1.12.2",
    "temporary": "0.0.8",
    "testem": "^0.5.15",
    "through2": "~0.6.3",
    "vinyl-buffer": "~1.0.0",
    "vinyl-source-stream": "^1.0.0"
  },
  "scripts": {
    "build": "sudo -u nyc-trees ./node_modules/.bin/gulp",
    "build-debug": "sudo -u nyc-trees ./node_modules/.bin/gulp build --debug",
    "watch": "sudo -u nyc-trees ./node_modules/.bin/gulp watch --debug",
    "watch-tests": "sudo -u nyc-trees ./node_modules/.bin/gulp watch-tests",
    "lint": "sudo -u nyc-trees ./node_modules/.bin/gulp lint"
  },
  "repository": {
    "type": "git",
    "url": "git://github.com/azavea/nyc-trees.git"
  },
  "author": "",
  "license": "Apache-2",
  "bugs": {
    "url": "https://github.com/azavea/nyc-trees/issues"
  }
}
